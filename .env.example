# Environment variables for the INARA platform.  Copy this file to
# `.env` and fill in the values before running locally.  When
# deploying to Vercel, these keys will be configured in the project
# settings or injected via the GitHub Actions workflow.

NEXTAUTH_SECRET=replace_me
NEXTAUTH_URL=https://your-vercel-domain

# Database connection string.  For Turso/libSQL, use the libsql://
# scheme and set DATABASE_AUTH_TOKEN.  For Neon Postgres, provide a
# PostgreSQL connection string instead and omit DATABASE_AUTH_TOKEN.
DATABASE_URL=libsql://<host>
DATABASE_AUTH_TOKEN=<turso_token>

# Feature gating
FREE_DAILY_PROJECTS=1
ROTATION_SEED=50
